{
  "name": "app",
  "private": "true",
  "scripts": {
    "build:css": "npx sass ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "watch:css": "npx sass --watch ./app/assets/stylesheets/application.bootstrap.scss:./app/assets/builds/application.css --load-path=node_modules"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "sass": "^1.94.2"
  }
}
